
---
---
{
  "short_name": "{{ site.short_name }}",
  "name": "{{ site.name }}",
  "icons": [
    {
      "src": "{{ site.github.baseurl }}/images/icon-192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "{{ site.github.baseurl }}/images/icon-512.png",
      "type": "image/png",
      "sizes": "512x512"
    },
    {
      "src": "{{ site.github.baseurl }}/images/icon.svg",
      "type": "image/svg",
      "sizes": "1600x1600"
    }
  ],
  "start_url": "{{ site.github.baseurl }}/?source=pwa",
  "background_color": "{{ site.theme_color or site.data.colors[3] }}",
  "display": "standalone",
  "scope": "{{ site.github.baseurl }}/",
  "theme_color": "{{ site.theme_color or site.data.colors[2] }}"
}
